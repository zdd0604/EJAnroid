<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_hj_about_informatio"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/htActivity"
        layout="@layout/actionbar_layout" />

    <ProgressBar
        android:id="@+id/progressBar_html"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_2"
        android:progressDrawable="@drawable/progressbar_html"
        android:layout_below="@id/htActivity"
        android:visibility="gone"/>

    <WebView
        android:id="@+id/wv_abouthj"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/progressBar_html"/>

    <include
        layout="@layout/layout_htmlloadererror"/>
</RelativeLayout>
